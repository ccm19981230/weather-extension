{
  "manifest_version": 2,

  "name": "Weather Extension",
  "description": "This extension shows the weather. ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}